{
    "type": "kafka",
    "spec": {
      "dataSchema": {
        "dataSource": "graze_events",
        "timestampSpec": {
          "column": "graze_time",
          "format": "iso"
        },
        "dimensionsSpec": {
          "dimensions": [
            "user1_id",
            "user2_id",
            "position.lat",
            "position.lng"
          ]
        }
      },
      "ioConfig": {
        "topic": "graze-events",
        "inputFormat": {
          "type": "json"
        },
        "consumerProperties": {
          "bootstrap.servers": "kafka:29092"
        },
        "taskCount": 1,
        "replicas": 1
      },
      "tuningConfig": {
        "type": "kafka"
      }
    }
  }
  